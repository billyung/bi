    <sql>select (ls.penalty+ls.misc_penalty) as misc_penalty_amount, ls.misc_fees as misc_fee_amount,
ls.action_date as action_date, ls.action_date as due_date,
ls.action_date as created_date,
a.customer_id, a.account_id as loan_account_id, if(ls.misc_fees, ls.misc_fees_currency_id, ls.misc_penalty_currency_id) as currency_id,
ls.installment_id

from loan_schedule ls
inner join account a on a.account_id = ls.account_id

where timestamp(ls.action_date) &gt;= date(?) 
and timestamp(ls.action_date) &lt; date(?)</sql>
