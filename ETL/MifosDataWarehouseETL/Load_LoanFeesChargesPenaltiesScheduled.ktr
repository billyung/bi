    <connection>SourceDB</connection>
    <sql>select ls.action_date as action_date, ls.action_date as due_date, ls.action_date as created_date,
lfs.amount,
a.customer_id, a.account_id as loan_account_id, lfs.amount_currency_id as currency_id, ls.installment_id,
1 as account_charge_type_id, lfs.fee_id

from loan_schedule ls
inner join loan_fee_schedule lfs on ls.id = lfs.id
inner join account a on a.account_id = ls.account_id

where timestamp(ls.action_date) &gt;= date(?) 
and timestamp(ls.action_date) &lt; date(?)</sql>
    <limit>0</limit>
    <lookup>Get Start and End Date Variables</lookup>
