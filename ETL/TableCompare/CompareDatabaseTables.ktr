    <connection>DB2</connection>
    <sql>
select k.table_name as table_field, group_concat(k.column_name) as key_fields_field,

&apos;last_modified, version&apos; as exclude_fields_field, &apos;&apos; as no_schema_field 
from information_schema.key_column_usage k
where  k.table_schema = schema() 
and k.table_name not like &apos;stg%&apos;
and k.constraint_name = &apos;PRIMARY&apos;
&#47;*and k.table_name not in (&apos;hist_loan_balances&apos;, &apos;hist_loan_arrears&apos;, &apos;hist_savings_balances&apos;)*&#47;
group by k.table_name
order by 1, 2</sql>
