    <connection>SourceDB</connection>
    <sql>select atxn.account_action_id, 
atxn.account_trxn_id, ftxn.fee_trxn_detail_id, a.customer_id as customer_id, atxn.amount_currency_id as currency_id, atxn.payment_id, atxn.installment_id, 
ifnull(atxn.due_date, date(&apos;3001-01-01&apos;)) due_date, atxn.action_date, date(atxn.created_date) created_date, ap.payment_type_id,
ftxn.fee_amount as amount, 1 as account_charge_type_id, af.fee_id
from account_trxn atxn
inner join fee_trxn_detail ftxn on atxn.account_trxn_id = ftxn.account_trxn_id
inner join account_payment ap on ap.payment_id = atxn.payment_id
inner join account a on a.account_id = atxn.account_id
left join account_fees af on af.account_fee_id = ftxn.account_fee_id
where atxn.account_action_id in (12, 13)  
and fee_amount &lt;&gt; 0
&#47;*
12 CustomerAccountRepayment
13 CustomerAdjustment
*&#47;
</sql>
