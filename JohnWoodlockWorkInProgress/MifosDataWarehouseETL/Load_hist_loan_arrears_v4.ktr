  <step>
    <name>Identify Dates to Process</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DestinationDB</connection>
    <sql>SELECT d.date_value as d1, d.date_value as d2, d.date_value as d3
from dim_date d
where d.date_value &lt; curdate()
and d.date_value &gt;=

    (select ifnull(min(valid_from), date(&apos;2199-12-31&apos;))
    from dim_loan 
    where loan_account_key &gt; 0) 

order by d.date_value</sql>
    <limit>20</limit>
    <lookup/>
