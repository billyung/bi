  <step>
    <name>Get Loan Accounts In Arrears As Of Date</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DestinationDB</connection>
    <sql>select loan.loan_account_id, paydate.date_value as as_of_date,

ifnull(due.principal_due, 0.0) as principal_due,
ifnull(due.interest_due, 0.0) as interest_due,
ifnull(paid.principal_paid, 0.0) as principal_paid,
ifnull(paid.interest_paid, 0.0) as interest_paid,

(ifnull(due.principal_due, 0.0) - ifnull(paid.principal_paid, 0.0)) as principal_in_arrears,
(ifnull(due.interest_due, 0.0) - ifnull(paid.interest_paid, 0.0)) as interest_in_arrears,
(ifnull(due.principal_due, 0.0) - ifnull(paid.principal_paid, 0.0)) + (ifnull(due.interest_due, 0.0) - ifnull(paid.interest_paid, 0.0)) as total_in_arrears
from dim_loan loan
join dim_date paydate on paydate.date_value = ? 

left join
    (select l.loan_account_id, paydate.date_value,
    ifnull(sum(ls.principal_amount), 0.0)  as principal_due,
    ifnull(sum(ls.interest_amount), 0.0)  as interest_due
    from dim_loan l
    join dim_date paydate on paydate.date_value = ? 
    join dw_loan_schedules ls on ls.loan_account_id = l.loan_account_id and ls.due_date &lt;= paydate.date_value
    where l.valid_from &lt;= paydate.date_value
    and l.valid_to &gt; paydate.date_value    
    and l.loan_status in (&apos;AccountState-ActiveInGoodStanding&apos;, &apos;AccountState-ActiveInBadStanding&apos;)
    group by l.loan_account_id, paydate.date_value) due on due.loan_account_id = loan.loan_account_id
                                                 and due.date_value = paydate.date_value 


left join
    (select l.loan_account_id, paydate.date_value,
    ifnull(sum(lr.principal_amount), 0.0) as principal_paid,
    ifnull(sum(lr.interest_amount), 0.0)  as interest_paid
    from dim_loan l
    join dim_date paydate on paydate.date_value = ?
    join fact_loan_repayments lr on lr.loan_account_id = l.loan_account_id and lr.action_date &lt;= paydate.date_value
&#47;* only pick up unadjusted repayments. i.e. adjustments (even though perhaps entered in the system after the arrears date being processed) are taken into account.
Reversals are ignored.  Although the reversal (aka cancel) transaction is backdated to the date of each entry it reverses... it doesn&apos;t come into play
until the day the reversal is entered on the system (indicated by a cancel loan account status).*&#47;
    join dim_account_action aa on aa.account_action_key = lr.account_action_key and aa.account_action_id = 1
      
    where l.valid_from &lt;= paydate.date_value 
    and l.valid_to &gt; paydate.date_value    
    and l.loan_status in (&apos;AccountState-ActiveInGoodStanding&apos;, &apos;AccountState-ActiveInBadStanding&apos;)
	and lr.adjusted is false
    group by l.loan_account_id, paydate.date_value) paid on paid.loan_account_id = loan.loan_account_id
                                                  and paid.date_value = paydate.date_value 

    where loan.valid_from &lt;= paydate.date_value
    and loan.valid_to &gt; paydate.date_value    
    and loan.loan_status in (&apos;AccountState-ActiveInGoodStanding&apos;, &apos;AccountState-ActiveInBadStanding&apos;)

    group by loan.loan_account_id, paydate.date_value 
    having (((principal_due + interest_due) - (principal_paid + interest_paid)) &gt; 0.0)
    order by loan.loan_account_id </sql>
    <limit>0</limit>
    <lookup>Identify Dates to Process</lookup>
    <execute_each_row>Y</execute_each_row>
    <variables_active>Y</variables_active>
