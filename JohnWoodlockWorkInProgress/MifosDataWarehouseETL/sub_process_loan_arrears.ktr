  <step>
    <name>Get Full Amounts Scheduled and Total Amount Expected up to Last Installment having Payment</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DestinationDB</connection>
    <rowlimit>1</rowlimit>
    <sql>&#47;*There must be at least one installment due*&#47;
select 
    ifnull(sum(ls.principal_amount), 0.0) as principal_scheduled,
    ifnull(sum(ls.interest_amount), 0.0)  as interest_scheduled,
    ifnull(sum(if (ls.installment_id &lt;= ?, (ls.principal_amount + ls.interest_amount), 0.0)), 0.0)  as total_amount_expected

from dw_loan_schedules ls
where ls.loan_account_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>N</replace_vars>
    <parameter>
      <field>
        <name>last_installment_having_payment</name>
        <type>Integer</type>
