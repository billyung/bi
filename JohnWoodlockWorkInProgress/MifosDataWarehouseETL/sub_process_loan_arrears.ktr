  <step>
    <name>Calculate Installment Due and In Arrears Values</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>N</compatible>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here

var installments_in_arrears;
var installment_id_due;


var outstanding_amount;

outstanding_amount = total_amount_expected - (principal_paid + interest_paid);

if (outstanding_amount &gt; 0.0)
{
	installment_id_due = last_installment_having_payment;
}
if (outstanding_amount == 0.0)
{
	installment_id_due = last_installment_having_payment + 1;
}
if (outstanding_amount &lt; 0.0)
{
	trans_Status = ERROR_TRANSFORMATION;
}

installments_in_arrears = (last_installment_due - installment_id_due) + 1;</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>installments_in_arrears</name>
