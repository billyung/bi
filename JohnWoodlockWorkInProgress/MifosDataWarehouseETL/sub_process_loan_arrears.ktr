  <step>
    <name>Get Last Installment that had a Payment</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DestinationDB</connection>
    <rowlimit>1</rowlimit>
    <sql>&#47;*There may not have been a payment*&#47;
select ifnull(max(r.installment_id),1) as last_installment_having_payment
from fact_loan_repayments r
join dim_account_action aa on aa.account_action_key = r.account_action_key
where r.loan_account_id = ?
and r.action_date &lt;= ?
and aa.account_action_id = 1
and adjusted is false</sql>
