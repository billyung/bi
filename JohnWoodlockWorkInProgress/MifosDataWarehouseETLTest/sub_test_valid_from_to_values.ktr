        <jsScript_script>function fail_test(msg)
{
	error_count = error_count + 1;
	if (error_count == 1)
	{
		test_result = &quot;Fail&quot;;
		test_msg = &quot;1st Error - &quot; + msg;
		Alert(msg);
	}
}

function process(tbl, col)
{

	DBConnection = &quot;DestinationDB&quot;;
	sql = &quot;select &quot; + col + &quot;_id, &quot; + col + &quot;_key, to_days(valid_from), to_days(valid_to), valid_from, valid_to &quot;;
	sql = sql + &quot; from dim_&quot; + tbl;
	sql = sql + &quot; order by 1, 2&quot;;
	sqlResults = fireToDB(DBConnection, sql);

	idcount = 0;
	keycount = 0;
	prev_id = -50;

	for (var i in sqlResults)
	{
		keycount = keycount + 1; 	

		curr_id = parseInt(sqlResults[i][0]);
		curr_key = parseInt(sqlResults[i][1]);
		curr_valid_from = parseInt(sqlResults[i][2]);
		curr_valid_to = parseInt(sqlResults[i][3]);
		curr_valid_from_string = sqlResults[i][4];
		curr_valid_to_string = sqlResults[i][5];

		if (curr_valid_from &gt; curr_valid_to)
		{
			fail_test(&quot;dim_&quot; + tbl + &quot;   id: &quot; + curr_id + &quot;  key: &quot; + curr_key + &quot;  valid_from: &quot; + curr_valid_from_string + &quot;  IS AFTER valid_to: &quot; + curr_valid_to_string);
		}
		

 
    	if (curr_id != prev_id)
		{ 
			idcount = idcount + 1;
			prev_id = curr_id;
		}
		else &#47;* can check if previous valid_to = current valid_from*&#47;
		{ 
			if (curr_valid_from != parseInt(sqlResults[i-1][3]))
			{
				fail_test(&quot;dim_&quot; + tbl + &quot;   id: &quot; + curr_id + &quot;  key: &quot; + curr_key + &quot;  current valid_from: &quot; + curr_valid_from_string + &quot;  DOESNT MATCH previous valid_to: &quot; + sqlResults[i-1][5]);
			}
		}
	}


	if (test_result == &quot;Pass&quot;)
	{
		test_msg = &quot;dim_&quot; + tbl + &quot;  Id Count: &quot; + idcount + &quot;  Key Count: &quot; + keycount;
	}
	else
	{
		test_msg = test_msg + &quot; -   Total Error Count: &quot; + error_count;
	}


}


var test_result = &quot;Pass&quot;;
var test_msg = &quot;&quot;;
var error_count = 0;

process(dim_table_name, column_prefix); 






