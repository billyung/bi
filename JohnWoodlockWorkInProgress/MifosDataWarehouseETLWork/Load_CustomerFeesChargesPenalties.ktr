    <connection>SourceDB</connection>
    <sql>select ctxn.misc_fee_amount, ctxn.misc_penalty_amount,
atxn.action_date, ifnull(atxn.due_date, date(&apos;3001-01-01&apos;)) due_date, date(atxn.created_date) created_date, atxn.account_action_id, 
atxn.account_trxn_id, a.customer_id as customer_id, atxn.amount_currency_id as currency_id, atxn.payment_id, atxn.installment_id, ap.payment_type_id
from account_trxn atxn
inner join customer_trxn_detail ctxn on atxn.account_trxn_id = ctxn.account_trxn_id
inner join account_payment ap on ap.payment_id = atxn.payment_id
inner join account a on a.account_id = atxn.account_id
where atxn.account_action_id in (12, 13)
and not (ctxn.misc_fee_amount = 0 and ctxn.misc_penalty_amount = 0)
&#47;*
12 CustomerAccountRepayment
13 CustomerAdjustment
*&#47;
</sql>
    <limit>0</limit>
