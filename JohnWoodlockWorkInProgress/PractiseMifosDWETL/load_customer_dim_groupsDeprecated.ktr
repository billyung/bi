  <step>
    <name>Extract New Groups</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>select c.customer_id, c.display_name, 
ifnull(cld.old_value, lv.lookup_name) as customer_status,
c.customer_level_id,
&#47;*if a group is not active it won&apos;t be in the hierarchy yet*&#47;
&#47;*also won&apos;t be in hierarchy if no parent *&#47;
ifnull(ifnull(ch.parent_id, c.parent_customer_id),0) as parent_id,  
0 as grandparent_id,
c.loan_officer_id, &#47;* use this later if group has no parent*&#47;
ifnull(c.customer_formedby_id, 0) as customer_formedby_id,
&apos;n&#47;a&apos; gender,
c.created_date
from customer c
join customer_state cs on cs.status_id = c.status_id
join lookup_value lv on lv.lookup_id = cs.status_lookup_id


&#47;* find first status if there has been a status change*&#47;
left join change_log cl on c.customer_id = cl.entity_id
                        and cl.entity_type = 12
                        and cl.change_log_id = 
                            (select min(clmin.change_log_id)
                            from change_log clmin
                            join change_log_detail cldmin on cldmin.change_log_id = clmin.change_log_id
                                                and cldmin.field_name =  &apos;Status&apos;
                            where clmin.entity_id = c.customer_id
                            and clmin.entity_type = 12)
left join change_log_detail cld on cld.change_log_id = cl.change_log_id
                        and cld.field_name =  &apos;Status&apos;
                        
&#47;*get parent from the hierarchy table*&#47;
left join customer_hierarchy ch on ch.customer_id = c.customer_id
                and ch.hierarchy_id = 
                    (select min(hierarchy_id) from customer_hierarchy chmin
                    where chmin.customer_id = c.customer_id)


where c.customer_level_id = 2
order by c.customer_id</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
