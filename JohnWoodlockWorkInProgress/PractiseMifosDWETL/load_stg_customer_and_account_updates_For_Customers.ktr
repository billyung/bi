    <connection>SourceDB</connection>
    <sql>
SELECT ch.start_date as effective_date, c.customer_level_id as level_id, c.customer_id as entity_id, 
&apos;H&apos; as change_type, ch.hierarchy_id as change_order, 
null as updated_status, ch.parent_id as updated_parent_id, null as updated_loan_officer_id, null as updated_branch_id,
c.display_name as customer_display_name, ifnull(c.customer_formedby_id, 0) as customer_formedby_id, 
ifnull(cdlv.lookup_name, &apos;Unknown&apos;) as customer_gender,
if(c.parent_customer_id is null, 1, 0) as customer_top_of_hierarchy

from customer c
join customer_hierarchy ch on ch.customer_id = c.customer_id
                and ch.hierarchy_id &gt; (select min(chmin.hierarchy_id)
                                                    from customer_hierarchy chmin
                                                    where chmin.customer_id = c.customer_id)
join customer_detail cd on cd.customer_id = c.customer_id
left join lookup_value cdlv on cdlv.lookup_id = cd.gender
where c.customer_level_id = 1


</sql>
