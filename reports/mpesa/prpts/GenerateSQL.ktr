    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
var SQL = &quot;select &apos;Amount&apos;, &apos;CreditIdentityString&apos;, &apos;CreditIdentityStringType&apos;, &apos;ValidationIdentityString&apos;, &apos;ValidationIdentityStringType&apos; union &quot;
SQL = SQL + &apos;select (disbursals.amount - ifnull(fees.fee_amount, 0.0)) as amount, &apos;;
SQL = SQL + &apos;case &apos;;
SQL = SQL + &quot;when disbursals.phone_number rlike &apos;^254[1-9][0-9]{8}$&apos; then disbursals.phone_number &quot;;
SQL = SQL + &quot;when disbursals.phone_number rlike &apos;^[1-9][0-9]{8}$&apos; then concat(&apos;254&apos;, disbursals.phone_number) &quot;;
SQL = SQL + &quot;when disbursals.phone_number rlike &apos;^[\\+0][1-9][0-9]{8}$&apos; then concat(&apos;254&apos;, substring(disbursals.phone_number, 2)) &quot;;
SQL = SQL + &apos;end &apos;;
SQL = SQL + &apos;as phone_number, &apos;;
SQL = SQL + &quot;&apos;[Phone Details][Mobile Phone Number]&apos; as identity_string, &quot;;
SQL = SQL + &quot;&apos;&apos; as validation_identity, &quot;;
SQL = SQL + &quot;&apos;[Security Information][National Identity Number]&apos; as validation_identity_type &quot;;
SQL = SQL + &apos;from &apos;;
SQL = SQL + &apos;(select &apos;;
SQL = SQL + &apos;a.account_id, &apos;;
SQL = SQL + &apos;loan_amount as amount, &apos;;
SQL = SQL + &apos;phone_number as phone_number &apos;;
SQL = SQL + &apos;from account a &apos;;
SQL = SQL + &apos;join loan_account la &apos;;
SQL = SQL + &apos;on la.account_id = a.account_id &apos;;
SQL = SQL + &apos;left join customer_address_detail cad &apos;;
SQL = SQL + &apos;on cad.customer_id = a.customer_id &apos;;
SQL = SQL + &apos;left join customer c &apos;;
SQL = SQL + &apos;on c.customer_id = a.customer_id &apos;;
SQL = SQL + &apos;where &apos;;
SQL = SQL + &apos;a.account_state_id = 3 &apos;;
SQL = SQL + &apos;and la.loan_amount &lt;= 50000 &apos;;
SQL = SQL + &quot;and (cad.phone_number is not null and (cad.phone_number rlike &apos;^254[1-9][0-9]{8}$&apos; or cad.phone_number rlike &apos;^[1-9][0-9]{8}$&apos; or cad.phone_number rlike &apos;^[\\+0][1-9][0-9]{8}$&apos;)) &quot;;
SQL = SQL + &apos;and la.disbursement_date = date(EXTRACT_DATE) &apos;;
SQL = SQL + &apos;and (c.parent_customer_id = C_GROUP or C_GROUP = -1) &apos;;
SQL = SQL + &apos;and (la.prd_offering_id = L_PRODUCT or L_PRODUCT = 0) &apos;;
SQL = SQL + &apos;) disbursals &apos;;
SQL = SQL + &apos;left join &apos;;
SQL = SQL + &apos;(select &apos;;
SQL = SQL + &apos;a.account_id, &apos;;
SQL = SQL + &apos;sum(ifnull(fee_amnt, 0.0)) as fee_amount &apos;;
SQL = SQL + &apos;from account a &apos;;
SQL = SQL + &apos;join loan_account la &apos;;
SQL = SQL + &apos;on la.account_id = a.account_id &apos;;
SQL = SQL + &apos;left join account_fees af &apos;;
SQL = SQL + &apos;on af.account_id = a.account_id &apos;;
SQL = SQL + &apos;left join fee_frequency ff &apos;;
SQL = SQL + &apos;on ff.fee_id = af.fee_id &apos;;
SQL = SQL + &apos;where &apos;;
SQL = SQL + &apos;a.account_state_id = 3 &apos;;
SQL = SQL + &apos;and ff.frequency_payment_id = 2 &apos;;
SQL = SQL + &apos;and la.loan_amount &lt;= 50000 &apos;;
SQL = SQL + &apos;and la.disbursement_date = date(EXTRACT_DATE) &apos;;
SQL = SQL + &apos;group by &apos;;
SQL = SQL + &apos;a.account_id &apos;;
SQL = SQL + &apos;) fees &apos;;
SQL = SQL + &apos;on fees.account_id = disbursals.account_id &apos;;

l_product = getVariable(&quot;l_product&quot;, &quot;&quot;);
extract_date = &quot;&apos;&quot; + getVariable(&quot;extract_date&quot;, &quot;&quot;) + &quot;&apos;&quot;;
c_group = getVariable(&quot;c_group&quot;, &quot;&quot;);

SQL = replace(SQL, &quot;L_PRODUCT&quot;, l_product);
SQL = replace(SQL, &quot;EXTRACT_DATE&quot;, extract_date);
SQL = replace(SQL, &quot;C_GROUP&quot;, c_group);

&#47;&#47; writeToLog(SQL);
&#47;&#47; Alert(SQL);


