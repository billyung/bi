    <sql>drop  table if exists arrears_accounts_temp;
create  table arrears_accounts_temp(loan_account_id int, branch_id smallint, 
loan_officer_id int,  customer_id int, prd_offering_id smallint,
  `as_of_date` date NOT NULL,
  `days_in_arrears` int(10) unsigned NOT NULL DEFAULT 0,
  `installments_in_arrears` int(10) unsigned NOT NULL DEFAULT 0,
  `principal_scheduled` decimal(21,4) NOT NULL DEFAULT &apos;0.0000&apos;,
  `interest_scheduled` decimal(21,4) NOT NULL DEFAULT &apos;0.0000&apos;,
  `principal_due` decimal(21,4) NOT NULL DEFAULT &apos;0.0000&apos;,
  `interest_due` decimal(21,4) NOT NULL DEFAULT &apos;0.0000&apos;,
  `principal_paid` decimal(21,4) NOT NULL DEFAULT &apos;0.0000&apos;,
  `interest_paid` decimal(21,4) NOT NULL DEFAULT &apos;0.0000&apos;,
  PRIMARY KEY (loan_account_id));

insert into arrears_accounts_temp(loan_account_id, branch_id, loan_officer_id,  customer_id, prd_offering_id, as_of_date)
select la.account_id as loan_account_id, a.office_id as branch_id, a.personnel_id as loan_officer_id, a.customer_id as customer_id,
la.prd_offering_id as prd_offering_id, date(&apos;${arrears_as_of_date}&apos;)
from loan_account la
join account a on a.account_id = la.account_id
where exists
(select &apos;f&apos; from account_status_change_history ch
where ch.account_id = la.account_id
and ch.new_status in (5,9)
and ch.changed_date &lt;= date(&apos;${arrears_as_of_date}&apos;)
and not exists
    (select &apos;f&apos; from account_status_change_history ch2
    where ch2.new_status not in (5,9)
    and ch2.changed_date &lt;= date(&apos;${arrears_as_of_date}&apos;)
    and ch2.account_id = la.account_id
    and ch2.account_status_change_id &gt; ch.account_status_change_id))

and exists
(select &apos;f&apos; from loan_schedule ls
where ls.account_id = la.account_id
and (ls.payment_date is null or ls.payment_date &gt; date(&apos;${arrears_as_of_date}&apos;))
and ls.action_date &lt; date(&apos;${arrears_as_of_date}&apos;));

&#47;* amounts scheduled *&#47;
update arrears_accounts_temp aat
set principal_scheduled =
 (select ifnull(sum(principal),0)
 from loan_schedule ls 
 where ls.account_id = aat.loan_account_id 
 );

update arrears_accounts_temp aat
set interest_scheduled =
 (select ifnull(sum(interest),0)
 from loan_schedule ls 
 where ls.account_id = aat.loan_account_id 
 );
 
&#47;* days and installments arrears *&#47;
update arrears_accounts_temp aat
set days_in_arrears =
 (select ifnull(to_days(date(&apos;${arrears_as_of_date}&apos;)) - to_days(max(ls.action_date)),0)
 from loan_schedule ls 
 where ls.account_id = aat.loan_account_id 
 and (ls.payment_date is null or ls.payment_date &gt; date(&apos;${arrears_as_of_date}&apos;))
 and ls.action_date &lt; date(&apos;${arrears_as_of_date}&apos;)
 );

update arrears_accounts_temp aat
set installments_in_arrears =
 (select count(ls.id)
 from loan_schedule ls 
 where ls.account_id = aat.loan_account_id 
 and (ls.payment_date is null or ls.payment_date &gt; date(&apos;${arrears_as_of_date}&apos;))
 and ls.action_date &lt; date(&apos;${arrears_as_of_date}&apos;)
 );
 
&#47;* amounts due *&#47;
update arrears_accounts_temp aat
set principal_due =
 (select ifnull(sum(principal),0)
 from loan_schedule ls 
 where ls.account_id = aat.loan_account_id 
 and ls.action_date &lt; date(&apos;${arrears_as_of_date}&apos;)
 );

update arrears_accounts_temp aat
set interest_due =
 (select ifnull(sum(interest),0)
 from loan_schedule ls 
 where ls.account_id = aat.loan_account_id 
 and ls.action_date &lt; date(&apos;${arrears_as_of_date}&apos;)
 );
 
 
&#47;* schedule totals*&#47;
update arrears_accounts_temp aat
set principal_paid =
 (select ifnull(sum(ltd.principal_amount),0)
 from account_trxn atr  
 join loan_trxn_detail ltd on ltd.account_trxn_id = atr.account_trxn_id
 where atr.account_id = aat.loan_account_id 
 and atr.account_action_id = 1
 and atr.action_date &lt; date(&apos;${arrears_as_of_date}&apos;)
 );

&#47;* paid totals *&#47;
update arrears_accounts_temp aat
set interest_paid =
 (select ifnull(sum(ltd.interest_amount),0)
 from account_trxn atr  
 join loan_trxn_detail ltd on ltd.account_trxn_id = atr.account_trxn_id
 where atr.account_id = aat.loan_account_id 
 and atr.account_action_id = 1
 and atr.action_date &lt; date(&apos;${arrears_as_of_date}&apos;)
 );</sql>
